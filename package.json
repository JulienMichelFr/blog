{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./public",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "cross-env NODE_ENV=production start-storybook",
    "build-storybook": "cross-env NODE_ENV=production build-storybook -s public",
    "publish": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@storybook/addon-knobs": "^5.2.8",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^6.0.3",
    "gatsby": "^2.18.12",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-typography": "^2.3.20",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.1",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
