{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./public ./.cache",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "cross-env NODE_ENV=production start-storybook",
    "build-storybook": "cross-env NODE_ENV=production build-storybook -s public",
    "publish": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@storybook/addon-knobs": "^5.3.10",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^6.0.3",
    "gatsby": "^2.19.12",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-typography": "^2.3.22",
    "gatsby-remark-embed-video": "^2.0.1",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.50",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.1",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.10",
    "@storybook/addon-links": "^5.3.10",
    "@storybook/addons": "^5.3.10",
    "@storybook/react": "^5.3.10",
    "babel-loader": "^8.0.6",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
